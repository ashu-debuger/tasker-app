  "version": "0.2.0",
  "configurations": [
    {
      "name": "tasker",
      "request": "launch",
      "type": "dart",
      "toolArgs": ["--dart-define=CLIQ_API_KEY=${env:CLIQ_API_KEY}"]
    },
    {
      "name": "tasker (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "toolArgs": ["--dart-define=CLIQ_API_KEY=${env:CLIQ_API_KEY}"]
    },
    {
      "name": "tasker (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": ["--dart-define=CLIQ_API_KEY=${env:CLIQ_API_KEY}"]
    }
  ]
}
